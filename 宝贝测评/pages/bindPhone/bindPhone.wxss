.bindwrapper{
  box-sizing: border-box;
  width: 100%;
  height: 1334rpx;
  padding-top: 352rpx;
  font-family: PingFangSC-Regular;
  overflow: hidden;
}
.section{
  position: relative;
  box-sizing: border-box;
  width: 78.4%;
  height: 100rpx;
  opacity: 0.35;
  background: #000000;
  border-radius: 50rpx;
  margin: 0 auto;
  overflow: hidden;
  z-index: 5;
}
.section image{
  position: absolute;
  top: 34rpx;
  left: 34rpx;
}
.section .telNum{
  position: absolute;
  width: 284rpx;
  height: 48rpx;
  line-height: 48rpx;
  font-size: 34rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  top: 24rpx;
  left: 90rpx;
}
.getCode{
  position: absolute;
  box-sizing: border-box;
  width: 160rpx;
  height: 40rpx;
  line-height: 40rpx;
  top: 28rpx;
  right: 60rpx;
  font-size: 28rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  padding-left: 12rpx;
  border-left: 5rpx solid #FFFFFF;
}
.sectionT{
  margin-top: 34rpx;
}
.sectionT image{
  top: 26rpx;
  left: 38rpx;
}
.btnSub{
   width: 78.4%;
   height: 100rpx;
   line-height: 100rpx;
   margin: 90rpx auto;
   border-radius: 50rpx;
   background: transparent;
   border: 2rpx solid #1AAD19;
   font-size: 36rpx;
   color: #1AAD19;
}
.home-page {
  height: 160rpx;
  width: 100%;
  border: 1rpx solid red;
  font-size: 60rpx;
}